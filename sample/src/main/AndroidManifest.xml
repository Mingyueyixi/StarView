<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yue.starview.excample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PathStartViewPre.ManyStarActivity"></activity>
        <activity android:name=".PathStartViewPre.RotateActivity"></activity>
        <activity android:name=".PathStartViewPre.StarSizeActivity"></activity>
        <activity android:name=".PathStartViewPre.DepthActivity"></activity>
        <activity android:name=".PathStartViewPre.StarHormActivity"/>
    </application>

</manifest>